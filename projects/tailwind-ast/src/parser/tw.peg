@export
TwParser = {statements:TwStatementNode} $;


TwStatementNode =
	  @:GroupNode
	| @:InstructNode
	;

GroupNode = head:InstructNode [Hyphen] '(' {statements:TwStatementNode} ')' [important:Important];

InstructNode = instructs:IdentifierNode {Hyphen instructs:IdentifierNode} [important:Important];

@char
Hyphen = '-';

@char
Important = '-';

@extern(crate::eat_css_class)
IdentifierNode;

@char
Dot = '.' | '。';

@char
Split = ';' | ',' | '，';